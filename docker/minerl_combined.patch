+    /**
+     * Apply time conditions from ServerInitialConditions
+     * Added for TimeInitialCondition support
+     */
+    private void applyTimeConditions(MissionInit missionInit) {
+        try {
+            ServerInitialConditions serverInit = getServerInit(missionInit);
+            if (serverInit == null || serverInit.getTime() == null) {
+                return;
+            }
+            
+            Time timeConfig = serverInit.getTime();
+            Integer startTime = timeConfig.getStartTime();
+            Boolean allowPassage = timeConfig.isAllowPassageOfTime();
+            
+            if (startTime != null) {
+                System.out.println("*** TimeInitialCondition: Setting world time to " + startTime + " ***");
+                
+                // Wait for server to be ready
+                Minecraft mc = Minecraft.getInstance();
+                MinecraftServer server = mc.getIntegratedServer();
+                
+                if (server != null) {
+                    for (ServerWorld world : server.getWorlds()) {
+                        world.setDayTime(startTime);
+                        System.out.println("*** Set day time to " + startTime + " for world: " + world.getDimensionKey().getLocation() + " ***");
+                    }
+                }
+            }
+            
+            if (allowPassage != null && !allowPassage) {
+                System.out.println("*** TimeInitialCondition: Disabling daylight cycle ***");
+                MinecraftServer server = Minecraft.getInstance().getIntegratedServer();
+                if (server != null) {
+                    server.getGameRules().get(GameRules.DO_DAYLIGHT_CYCLE).set(false, server);
+                }
+            }
+        } catch (Exception e) {
+            LOGGER.error("Error applying time conditions: ", e);
+        }
+    }
